{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"paths": {
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"],
      "$assets/*": ["./src/assets/*"],
      "$components/*": ["./src/components/*"],
      "$middlewares/*": ["./src/middlewares/*"],
    }
	}
}
